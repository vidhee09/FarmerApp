<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.FarmerApp"
        tools:targetApi="31">
        <activity
            android:name="com.ananta.fieldAgent.Activity.AddRequestActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.JointReportActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.ServiceReportActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.DeliveryReportActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.SitInspectionReportActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.FarmerDetailActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.LoginScreen"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.verifyOTPScreen"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.MainActivity"
            android:exported="false" />
        <activity
            android:name="com.ananta.fieldAgent.Activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>